        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@mui/material": "^5.15.2",
        "chart.js": "^4.4.1",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.2.0",
        "validator": "^13.11.0"
    }
