{
    "private": true,
    "prettier": {
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "tabWidth": 2,
        "endOfLine": "auto"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
